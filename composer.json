{
  "name": "leebrooks3/php",
  "description": "A library of core php things.",
  "keywords": ["php"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3",
    "nesbot/carbon": "^1.33"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.7",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "LeeBrooks3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LeeBrooks3\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
